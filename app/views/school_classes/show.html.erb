<div class="container">
  <h1><%= "Class #{@school_class.level}#{@school_class.indication}" %></h1>
  <%= link_to "Back to classes list", school_classes_path %>
  <h1>Students in class</h1>
  <table class="table">
    <tr>
      <th>Student</th>
    </tr>
    <% @school_class.students.each do |student| %>
        <tr>
          <td><%= link_to "#{student.first_name} #{student.last_name}", student_path(student.id) %></td>
        </tr>
    <% end %>
  </table>

</div>